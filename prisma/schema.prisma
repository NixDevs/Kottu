datasource db {
    provider    = "sqlite"
    url         = "file:data.db"
}
generator client {
  provider = "prisma-client-js"
}
model Guild {
    id          String @id @unique @map("_id")
    adminRole   String 
    blacklisted Boolean
}
model User {
    id String @id @map("_id") @unique
    blacklisted Boolean
    stats String
}